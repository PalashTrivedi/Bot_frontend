{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ],
    "paths": {
      "AdminServices/*": [
        "app/admin/services/*"
      ],
      "AdminPages/*": [
        "app/admin/pages/*"
      ],
      "AdminLayout/*": [
        "app/admin/layout/*"
      ],
      "AdminGuards/*": [
        "app/admin/guards/*"
      ],
      "AdminInterceptors/*": [
        "app/admin/interceptors/*"
      ],
      
      "AuthPages/*": [
        "app/auth/pages/*"
      ],
      "AuthServices/*": [
        "app/auth/services/*"
      ],
      "AuthLayout/*": [
        "app/auth/auth-layout/*"
      ],
      "CommonServices/*": [
        "app/common/services/*"
      ],
      "CommonGuards/*": [
        "app/common/guards/*"
      ],
      "CommonModels/*": [
        "app/common/models/*"
      ],
      "CommonInterceptors/*": [
        "app/common/interceptors/*"
      ],
    }
  }
}
